PROJECT_NAME=ISACC

SITE_ID=

# OIDC_CLIENT_SECRET must match __KEYCLOAK_FEMR_REALM_FEMR_OPENID_CLIENT_SECRET
OIDC_CLIENT_SECRET=
SECRET_KEY=
